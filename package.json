{
  "name": "runst",
  "displayName": "runst",
  "description": "Quick execution of Rust tests",
  "publisher": "fcoury",
  "repository": {
    "type": "git",
    "url": "https://github.com/fcoury/runst"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "runst.runCurrentTest",
        "title": "Runst: Execute current test"
      },
      {
        "command": "runst.runLastTest",
        "title": "Runst: Execute last test"
      }
    ],
    "keybindings": [
      {
        "command": "runst.runCurrentTest",
        "key": "shift+win+a",
        "mac": "shift+cmd+a"
      },
      {
        "command": "runst.runCurrentTest",
        "key": "ctrl+win+a",
        "mac": "ctrl+cmd+a"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  }
}
